<tr>   
  <td><%=title %></td>
  <% total_count = 0 %>
  <% active_user_count = 0 %>
  <% (0..6).reverse_each do |i| %>
    <td class="dash-table-content"> 
      <%= data[i] %> 
      <% if title == 'new User' %>
        (<%= active[i] %>)
        <% active_user_count += active[i]  %>
      <% end %>
      <% total_count += data[i] %>
    </td>
  <% end %>
  <td class="table-total"><%= total_count %>
  <% if title == 'new User' %>
    (<%= active_user_count %>)
  <% end %>
</td>
</tr>

