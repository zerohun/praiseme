<div id="dash-body">
  <div id="dash-menu">
    <ul class="nav nav-pills nav-stacked">
      User 
      <li><%= link_to 'All User', '#', :remote => true %> </li>
      <li><%= link_to 'Active User', '#', :remote => true %> </li>
      <li><%= link_to 'NonActive User', '#' %> </li>
      <li class="active"><%= link_to 'New User', '#' %> </li>
    </ul>
    <hr />
    <ul class="nav nav-pills nav-stacked">
      Stamp
      <li><%= link_to 'Stamp Rank', '' %> </li>
      <li><%= link_to 'New Stamp', '' %> </li>
    </ul>
    <hr />
    <ul class="nav nav-pills nav-stacked">
      Compliment
      <li><%= link_to 'Two day Ago', '' %> </li>
      <li><%= link_to 'Yesterday', '' %> </li>
      <li><%= link_to 'Today', '' %> </li>
      <li><%= link_to 1.months.ago.strftime("%B"), '' %> </li>
      <li class><%= link_to Date.today.strftime("%B"), '' %> </li>
      <li><%#= link_to 1.month.from_now.strftime("%B"), '' %> </li>
    </ul>
    <hr />
  </div>
  
  <div id="dash-content" >
    <% @compliment.each do |com| %>
      <%= com.sender.username %>
      <%= com.receiver.username %>
      <%= com.description %>
      <br />
    <%end %>
    new Compli : <%= @new_compliment.count %><br/>
    new USer : <%= @new_user.count %>
  </div>
</div>
