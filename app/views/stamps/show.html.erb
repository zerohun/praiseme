<div id="stamp-show" class="round border">
  <p id="notice"><%= notice %></p>
  <%= render @stamp %>
  <h5><%= @stamp.user_stamps.count %> people have this glory</h5>
</div>

<% if current_user.present? && current_user.user_stamps.where(:stamp_id => params[:id]).present? %>
  <%= render :partial => 'stamp_user', :locals => {:user => current_user, :is_mine => true, :stamp_id => @stamp.id} %>
<% end %>

<div id='friends'>
  <%= render :partial => 'stamp_user', :locals => {:users => @users, :is_mine => false, :stamp_id => @stamp.id} %>
</div>
<%= paginate @users %>
